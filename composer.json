{
    "name": "acseo/select-autocomplete-bundle",
    "type": "symfony-bundle",
    "description": "Make Symfony form autocompletion easily",
    "keywords": ["Autocomplete", "Select", "FormType", "Symfony", "Doctrine", "Bundle", "Acseo"],
    "homepage": "https://github.com/acseo/select-autocomplete-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Marcachi",
            "email": "brandon.marcachi@acseo-conseil.fr"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0",
        "symfony/form": "^3.4 || ^4.0 || ^5.0",
        "symfony/serializer": "^3.4 || ^4.0 || ^5.0",
        "symfony/property-access": "^3.4 || ^4.0 || ^5.0",
        "symfony/flex": "^1.3.0"
    },
    "require-dev": {
        "symfony/config": "^3.4 || ^4.0 || ^5.0",
        "doctrine/doctrine-bundle": "^1.6 || ^2.0",
        "symfony/console": "^3.4 || ^4.0 || ^5.0",
        "symfony/yaml": "^3.4 || ^4.0 || ^5.0",
        "symfony/framework-bundle": "^3.4 || ^4.0 || ^5.0",
        "sensio/framework-extra-bundle": "^3.4 || ^4.0 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.4 || ^5.0",
        "symfony/phpunit-bridge": "^3.4 || 4.3 || ^5.0",
        "symfony/dotenv": "^3.4 || ^4.0 || ^5.0",
        "friendsofphp/php-cs-fixer": "^2.13",
        "jangregor/phpstan-prophecy": "^0.2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "phpstan/phpstan-symfony": "^0.10.2",
        "doctrine/mongodb-odm-bundle": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Acseo\\SelectAutocomplete\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acseo\\SelectAutocomplete\\Tests\\": "tests/",
            "Acseo\\SelectAutocomplete\\Tests\\App\\": "tests/App/src/"
        }
    },
    "minimum-stability": "stable"
}
